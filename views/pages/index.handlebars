
<div class="card">
    <h5 class="card-header">Registered Users</h5>
    <div class="card-body">
        <ul class="list-group">
            {{#each users}}
                <li class="list-group-item">{{username}}</li>
            {{else}}
            <div class="alert alert-info">
                There is No Registered Users!
            </div>
            {{/each}}
        </ul>
    </div>
    {{#if user}}
        <h4>Aktif Kullanici : {{user}}</h4>
    {{/if}}
</div>